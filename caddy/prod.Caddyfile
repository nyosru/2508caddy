(base) {
    # Формируем самоподписной сертификат для работы https
    # tls support@php-cat.com
    #tls internal

    file_server
    encode gzip
}

(tls1) {
    tls {
        issuer acme {
            dir https://acme-v02.api.letsencrypt.org/directory
            email support@php-cat.com
        }

    }
}

(tls11) {
    tls {
        issuer acme {
            dir https://acme-v02.api.letsencrypt.org/directory
            email support@php-cat.com
        }
        # Отключение OCSP stapling
        ocsp_stapling off
    }
}

(tls_off) {
    tls internal
}

